<%- include("../templates/encabezado") %>
<%- include("../templates/menu") %>
<div class="container mt-5">
    <form action="/nuevousuario" method="post" enctype="multipart/form-data">
        <div class="card">
            <div class="card-header">
                <h1>NUEVO USUARIO</h1>
            </div>
            <div class="card-body">
                <input class="form-control mb-3" type="test" name="nombre" id="nombre" placeholder="Nombre" autofocus>
                <input class="form-control mb-3" type="test" name="usuario" id="usuario" placeholder="Usuario">
                <input class="form-control mb-3" type="test" name="password" id="password" placeholder="Password">
                <input class="form-control mb-3" type="file" name="foto" id="foto">
            </div>
            <div class="card-footer">
                <button class="form-control btn btn-primary" >Guardar Usuario</button>
            </div>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
        </div>
    </form>
</div>
